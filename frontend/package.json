{
  "name": "frontend",
  "version": "4.0.0",
  "description": "React try run with three domains",
  "scripts": {
    "dev1": "parcel serve src/index1.html -p 8081 --open",
    "dev2": "parcel serve src/index2.html -p 8082 --open",
    "dev3": "parcel serve src/index3.html -p 8083 --open",
    "build1": "parcel build --dist-dir public/index1 src/index1.html",
    "build2": "parcel build --dist-dir public/index2 src/index2.html",
    "build3": "parcel build --dist-dir public/index3 src/index3.html",
    "clean": "rm -rf .parcel-cache dist",
    "serve": "pnpx serve dist",
    "start": "pnpm clean && pnpm build && pnpm serve"
  },
  "devDependencies": {
    "@parcel/transformer-image": "2.12.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "parcel": "2.12.0",
    "postcss": "8.4.38",
    "process": "0.11.10",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "postcss-import": "16.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.23.0",
    "tailwindcss": "3.4.3"
  }
}
